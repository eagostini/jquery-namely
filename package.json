{
  "name": "jquery-namely",
  "version": "1.1.1",
  "description": "A jQuery by Name Selector Extension",
  "main": "src/jquery-namely.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dist": "npm install && npm test && npm run dev && npm run production",
    "watch": "NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "test": "nyc ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eagostini/jquery-namely.git"
  },
  "keywords": [
    "jquery",
    "jquery-plugin",
    "selector",
    "at",
    "name"
  ],
  "author": {
    "name": "Eduardo Agostini",
    "email": "edu.agostini@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eagostini/jquery-namely/issues"
  },
  "homepage": "https://github.com/eagostini/jquery-namely#readme",
  "devDependencies": {
    "ava": "^0.21.0",
    "jquery": "^3.2.1",
    "jsdom": "^11.1.0",
    "laravel-mix": "^1.4.2",
    "nyc": "^11.1.0"
  }
}
